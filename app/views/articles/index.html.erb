<div class="container w-50">
  <div class="ms-5 p-3">
    <h2>Articles</h2>
    <ul class="list-unstyled ms-2">
      <% @articles.each do |article| %>
        <li class="mb-2"><%= link_to article.title, article_path(article), class: "text-decoration-none fs-4 text-info" %></li>
      <% end %>
    </ul>
  </div>

</div>
